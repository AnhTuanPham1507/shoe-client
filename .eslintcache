tton>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div className=\"catalog__filter__toggle\">\r\n                    <Button size=\"sm\" onClick={() => showHideFilter()}>bộ lọc</Button>\r\n                </div>\r\n                <div className=\"catalog__content\">\r\n\r\n                    <InfinityList\r\n                        products={products}                     \r\n                    />\r\n                    {\r\n                        products.length > 0 &&\r\n                        <ReactPaginate\r\n                        className=\"pagination\"\r\n                        breakLabel=\"...\"\r\n                        nextLabel=\">\"\r\n                        onPageChange={handlePageClick}\r\n                        pageRangeDisplayed={8}\r\n                        pageCount={pageCount? pageCount: 0}\r\n                        previousLabel=\"<\"\r\n                        renderOnZeroPageCount={null}\r\n                    />\r\n                    }                 \r\n                   \r\n                </div>\r\n            </div>      \r\n     \r\n            </Container> \r\n        </Helmet>\r\n    )\r\n}\r\n\r\nexport default Catalog\r\n","C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\components\\InfinityList.jsx",[],"C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\components\\CheckBox.jsx",[],"C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\hooks\\useQuery.jsx",[],"C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\components\\Search.jsx",[],"C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\pages\\Product.jsx",[],"C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\components\\ProductView.jsx",[],"C:\\Users\\phama\\OneDrive\\Desktop\\source-code\\human\\human-client-site\\src\\compo